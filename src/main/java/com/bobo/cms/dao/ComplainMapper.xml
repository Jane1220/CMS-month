<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bobo.cms.dao.ComplainMapper" >
  <insert id="insert">
  insert into cms_complain(content,user_id,created,article_id,picurl) 
  values(#{content},#{user_id},#{created},#{articleId},#{picurl})
  
  </insert>
</mapper>